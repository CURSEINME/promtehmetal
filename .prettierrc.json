{
	"plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],

	"tabWidth": 2,
	"printWidth": 85,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"semi": false,
	"bracketSpacing": true,
	"trailingComma": "none",
	"useTabs": true,
	"proseWrap": "always",
	"jsxBracketSameLine": false
}
