datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
type Advantages {
  title String
  description String
}
model services {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  slug           String
  title          String
  description    String
  serviceFor     String
  subDesc        String
  advantages     Advantages[]
  productImage   String
  serviceImage   String
  serviceIcon    String
}

model User {
  id            String          @id @default(auto()) @map("_id") @db.ObjectId
  name          String
  password      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}